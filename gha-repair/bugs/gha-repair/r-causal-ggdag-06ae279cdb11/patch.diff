-    
-
-    <h2 class="hasAnchor" id="examples"><a class="anchor" href="#examples"></a>Examples</h2>
-    <pre class="examples"><div class='input'>
-<span class='fu'>dagify</span>(<span class='no'>y</span> ~ <span class='no'>x</span> + <span class='no'>z</span>, <span class='no'>x</span>~ <span class='no'>z</span>)</div><div class='output co'>#&gt; [1] "dag {\nx\ny\nz\nx -&gt; y\nz -&gt; x\nz -&gt; y\n}\n"
-#&gt; attr(,"class")
-#&gt; [1] "dagitty"</div><div class='input'>
-<span class='no'>coords</span> <span class='kw'>&lt;-</span> <span class='fu'><a href='https://www.rdocumentation.org/packages/base/topics/list'>list</a></span>(
-    <span class='kw'>x</span> <span class='kw'>=</span> <span class='fu'><a href='https://www.rdocumentation.org/packages/base/topics/c'>c</a></span>(<span class='kw'>A</span> <span class='kw'>=</span> <span class='fl'>1</span>, <span class='kw'>B</span> <span class='kw'>=</span> <span class='fl'>2</span>, <span class='kw'>D</span> <span class='kw'>=</span> <span class='fl'>3</span>, <span class='kw'>C</span> <span class='kw'>=</span> <span class='fl'>3</span>, <span class='kw'>F</span> <span class='kw'>=</span> <span class='fl'>3</span>, <span class='kw'>E</span> <span class='kw'>=</span> <span class='fl'>4</span>, <span class='kw'>G</span> <span class='kw'>=</span> <span class='fl'>5</span>, <span class='kw'>H</span> <span class='kw'>=</span> <span class='fl'>5</span>, <span class='kw'>I</span> <span class='kw'>=</span> <span class='fl'>5</span>),
-    <span class='kw'>y</span> <span class='kw'>=</span> <span class='fu'><a href='https://www.rdocumentation.org/packages/base/topics/c'>c</a></span>(<span class='kw'>A</span> <span class='kw'>=</span> <span class='fl'>0</span>, <span class='kw'>B</span> <span class='kw'>=</span> <span class='fl'>0</span>, <span class='kw'>D</span> <span class='kw'>=</span> <span class='fl'>1</span>, <span class='kw'>C</span> <span class='kw'>=</span> <span class='fl'>0</span>, <span class='kw'>F</span> <span class='kw'>=</span> -<span class='fl'>1</span>, <span class='kw'>E</span> <span class='kw'>=</span> <span class='fl'>0</span>, <span class='kw'>G</span> <span class='kw'>=</span> <span class='fl'>1</span>, <span class='kw'>H</span> <span class='kw'>=</span> <span class='fl'>0</span>, <span class='kw'>I</span> <span class='kw'>=</span> -<span class='fl'>1</span>)
-    )
-
-<span class='no'>dag</span> <span class='kw'>&lt;-</span> <span class='fu'>dagify</span>(<span class='no'>G</span> ~~ <span class='no'>H</span>,
-       <span class='no'>G</span> ~~ <span class='no'>I</span>,
-       <span class='no'>I</span> ~~ <span class='no'>G</span>,
-       <span class='no'>H</span> ~~ <span class='no'>I</span>,
-       <span class='no'>D</span> ~ <span class='no'>B</span>,
-       <span class='no'>C</span> ~ <span class='no'>B</span>,
-       <span class='no'>I</span> ~ <span class='no'>C</span> + <span class='no'>F</span>,
-       <span class='no'>F</span> ~ <span class='no'>B</span>,
-       <span class='no'>B</span> ~ <span class='no'>A</span>,
-       <span class='no'>H</span> ~ <span class='no'>E</span>,
-       <span class='no'>C</span> ~ <span class='no'>E</span> + <span class='no'>G</span>,
-       <span class='no'>G</span> ~ <span class='no'>D</span>, <span class='kw'>coords</span> <span class='kw'>=</span> <span class='no'>coords</span>)
-
-<span class='kw pkg'>dagitty</span><span class='kw ns'>::</span><span class='fu'><a href='https://www.rdocumentation.org/packages/dagitty/topics/is.dagitty'>is.dagitty</a></span>(<span class='no'>dag</span>)</div><div class='output co'>#&gt; [1] TRUE</div><div class='input'>
